    "exit_rules": {
        "long": "RSI reaches 70 or stop-loss hit",
        "short": "RSI reaches 30 or stop-loss hit"
    },
    "parameters": {
        "rsi_period": 14,
        "oversold": 30,
        "overbought": 70
    }
